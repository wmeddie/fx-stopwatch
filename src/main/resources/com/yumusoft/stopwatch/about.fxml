<GridPane fx:id="root" hgap="14.0" maxHeight="170.0" maxWidth="300.0" minHeight="170.0" minWidth="300.0" prefHeight="170.0" prefWidth="300.0" vgap="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.yumusoft.stopwatch.AboutController">
  <children>
    <VBox maxHeight="+Infinity" maxWidth="+Infinity" minHeight="-Infinity" prefWidth="177.0" spacing="7.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
      <children>
        <Label fx:id="messageLabel" alignment="CENTER" contentDisplay="CENTER" prefWidth="257.0" text="Stopwatch" textAlignment="CENTER" wrapText="true">
          <font>
            <Font name="System Bold" size="13.0" />
