            <Font size="12.0" />
          </font>
        </Label>
        <Label prefWidth="257.0" text="Copyright 2013 Eduardo Gonzalez" />
      </children>
    </VBox>
    <Button fx:id="okButton" minWidth="80.0" mnemonicParsing="false" onAction="#doOk" text="Ok" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
  </children>
  <columnConstraints>
    <ColumnConstraints hgrow="NEVER" maxWidth="-Infinity" minWidth="-Infinity" />
    <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" maxWidth="+Infinity" minWidth="-Infinity" />
  </columnConstraints>
  <padding>
    <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
  </padding>
  <rowConstraints>
    <RowConstraints maxHeight="+Infinity" minHeight="-Infinity" prefHeight="0.0" valignment="CENTER" vgrow="ALWAYS" />
    <RowConstraints maxHeight="+Infinity" minHeight="-Infinity" prefHeight="61.0" valignment="CENTER" vgrow="ALWAYS" />
    <RowConstraints maxHeight="+Infinity" minHeight="-Infinity" prefHeight="21.0" valignment="CENTER" vgrow="ALWAYS" />
    <RowConstraints maxHeight="0.0" minHeight="-Infinity" prefHeight="0.0" vgrow="NEVER" />
  </rowConstraints>
</GridPane>
